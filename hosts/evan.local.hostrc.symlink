# NPM completion
eval "$(npm completion 2>/dev/null)"

# Names
export DEFAULT_USER=evan

# Impromptu prompt
export IMPROMPTU_UNIX_DOMAIN_SOCKET="${HOME}/.impromptu/tmp/impromptu.sock"
source impromptu

# Docker Machine
DOCKER_MACHINE_NAME="dev"
export DEFAULT_DOCKER_MACHINE_NAME="${DOCKER_MACHINE_NAME}"
eval "$(docker-machine env ${DOCKER_MACHINE_NAME})"
