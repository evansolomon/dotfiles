# NPM completion
eval "$(npm completion 2>/dev/null)"

# Homebrew's PHP and Python
export PATH="$(brew --prefix php54)/bin:$(brew --prefix)/share/python:$PATH"

# Names
export DEFAULT_USER=evan

# Impromptu prompt
export IMPROMPTU_UNIX_DOMAIN_SOCKET="${HOME}/.impromptu/tmp/impromptu.sock"
source impromptu
